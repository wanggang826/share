page {
  width: 100%;
  height: 100%;
  background: #eee;
}

scroll-view {
  width: 100%;
}

.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #eee;
}

.content {
  width: 100%;
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
  color: #2a2929;
}

.titleclosed {
  height: 80rpx;
  width: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
  background: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 2rpx;
}

.title {
  height: 80rpx;
  width: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
  background: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.container image {
  width: 30rpx;
  height: 30rpx;
}

.answer {
  width: 100%;
  padding: 32rpx 24rpx;
  box-sizing: border-box;
  line-height: 36rpx;
}

.btn {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.btn button {
  background: linear-gradient(#ffd500, #feda02);
  color: #252525;
  font-size: 32rpx;
  height: 80rpx;
  width: 556rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 40rpx 0 76rpx 0;
}
