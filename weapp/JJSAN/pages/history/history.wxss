  padding-top: 22rpx;
}

.open .content >text, .open .content>view {
  margin: 0 0 22rpx 20rpx;
  color: #2a2929;
  font-size: 28rpx;
}

.open .tips {
  display: flex;
  align-items: center;
}

.open .tips view {
  color: #8d8d8d;
  margin-right: 50rpx;
}

.open .tips image {
