page {
  height: 100%;
  width: 100%;
  background: #eee;
}

.scroll{
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.box {
  width: 100%;
  margin-bottom: 20rpx;
}

.closed {
  background: white;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding-bottom: 22rpx;
}

.closed>view {
  margin-top: 22rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.closed .title {
  justify-content: space-between;
}

.closed .title >image {
  width: 36rpx;
  height: 36rpx;
}

.closed .title >view {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}

.closed .title .time {
  font-size: 28rpx;
  color: #2a2929;
}

.closed .title .remind {
  width: 110rpx;
  height: 28rpx;
  font-size: 24rpx;
  margin-left: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 20rpx;
  color: #fff;
}

.closed .cost, .closed .number {
  font-size: 26rpx;
  color: #8d8d8d;
}

.closed .sum {
  font-size: 36rpx;
  color: #f5c800;
  margin-left: 20rpx;
}

.open {
  display: flex;
  flex-direction: column;
  background: #eee;
}

.open .title {
  display: flex;
  justify-content: space-between;
  padding: 22rpx 20rpx;
  background: white;
  margin-bottom: 2rpx;
}

.open .title>text {
